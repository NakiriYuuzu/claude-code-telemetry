{
  "event_type": "claude_code.api_request",
  "required_attributes": [
    "session.id",
    "model",
    "input_tokens",
    "output_tokens",
    "cost_usd",
    "duration_ms"
  ],
  "validation": {
    "model": {
      "type": "string",
      "pattern": "^claude-[0-9]+-[a-z]+-[0-9]+$"
    },
    "input_tokens": {
      "type": "string",
      "pattern": "^[0-9]+$"
    },
    "output_tokens": {
      "type": "string",
      "pattern": "^[0-9]+$"
    },
    "cost_usd": {
      "type": "string",
      "pattern": "^[0-9]+\\.[0-9]+$"
    },
    "duration_ms": {
      "type": "string",
      "pattern": "^[0-9]+$"
    }
  }
}